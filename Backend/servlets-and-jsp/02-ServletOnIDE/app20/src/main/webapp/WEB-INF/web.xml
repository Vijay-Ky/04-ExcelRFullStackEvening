<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>app20</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>mainServlet</servlet-name>
  	<servlet-class>com.excelr.main.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>redirectedServlet</servlet-name>
  	<servlet-class>com.excelr.main.RedirectedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>mainServlet</servlet-name>
  	<url-pattern>/mainServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>redirectedServlet</servlet-name>
  	<url-pattern>/redirectedServlet</url-pattern>
  </servlet-mapping>
</web-app>